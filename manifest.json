{
  "name": "To Do List",
  "version": "0.4",
  "description": "To Do List - Chrome extension enables you organize your tasks effectively right from your browser.",
   "icons": {
    "128": "icon.png",
    "48": "icon.png",
    "16": "icon.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": ["management","cookies"],
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'"
}
